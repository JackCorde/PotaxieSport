@{
    ViewData["Title"] = "Coach";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
    @model List<PotaxieSport.Models.Usuario>


}

<div class="container table-container">
    <table class="table table-hover">
        <thead>
            <tr>
                <th>ID</th>
                <th>Nombre</th>
                <th>Apellido Paterno</th>
                <th>Apellido Materno</th>
                <th>Nombre de Usuario</th>
                <th>Email</th>
                <th>Rol</th>
                <th>Ver</th>
                <th>Editar</th>
                <th>Eliminar</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var administrador in Model)
            {
                <tr>
                    <td>@administrador.UsuarioId</td>
                    <td>@administrador.Nombre</td>
                    <td>@administrador.ApPaterno</td>
                    <td>@administrador.ApMaterno</td>
                    <td>@administrador.Username</td>
                    <td>@administrador.Email</td>
                    <td>@administrador.Rol</td>
                    <td class="action-icons"><a href="#"><i class="fas fa-eye"></i></a></td>
                    <td class="action-icons"><a href="#"><i class="fas fa-edit"></i></a></td>
                    <td class="action-icons"><a href="#"><i class="fas fa-trash-alt"></i></a></td>
                </tr>
            }
        </tbody>
    </table>
</div>
